    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "codelyzer": "^1.0.0-beta.2",
    "conventional-changelog-cli": "^1.2.0",
    "copy-webpack-plugin": "^3.0.1",
    "copyfiles": "^1.0.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "css-to-string-loader": "^0.1.2",

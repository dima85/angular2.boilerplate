{
  "name": "angular2.boilerplate",
  "version": "0.0.1-beta.1",
  "description": "Angular2 boilerplate project with webpack and karma",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist typings coverage",
    "dev": "webpack-dev-server --inline --hot --progress --port 8080 --open",
    "prebuild": "rimraf dist && npm run lint",
    "build": "webpack --config config/webpack.prod.js --progress --profile -bail",
    "devserver": "nodemon src/server.js",
    "lint": "tslint -c tslint.json ./src/**/*.ts -t verbose",
    "typings": "typings",
    "install-types": "typings install",
    "test": "karma start",
    "tdd": "karma start --no-single-run --auto-watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist"
  ],
  "author": "dshykaliuk@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "@angular/upgrade": "~2.1.0",
    "Base64": "^1.0.0",
